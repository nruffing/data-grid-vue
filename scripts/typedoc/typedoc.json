{
  "$schema": "https://typedoc.org/schema.json",
  "tsconfig": "./tsconfig.typedoc.json",
  "name": "Documentation",
  "cleanOutputDir": true,
  "entryPoints": ["../../dist/types/main.d.ts"],
  "out": "../../vuepress/docs/generated",
  "plugin": ["./typedoc-vue.js", "typedoc-plugin-markdown", "typedoc-plugin-mdn-links"],
  "readme": "none",
  "hideBreadcrumbs": true,
  "groupOrder": [
    "Data Grid Component",
    "Components",
    "Column",
    "Filter",
    "Sort",
    "Data Service",
    "Storage Service",
    "props",
    "emits",
    "slots",
    "Properties",
    "*"
  ],
  "sort": ["source-order"],
  "useCodeBlocks": true,
  "expandObjects": true,
  "parametersFormat": "table",
  "hidePageHeader": true,
  "typeDeclarationFormat": "table",
  "enumMembersFormat": "table",
  "propertiesFormat": "table",
  "disableSources": true,
  "hideInPageTOC": true,
  "json": "typedoc-out.json"
}
